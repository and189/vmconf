          "metricColumn": "none",
          "rawQuery": true,
          "rawSql": "SELECT\nvm.deviceName,\nifnull(vm.ip,'no_data') as 'ip',\nifnull(vm.mace,'no_data') as 'MacId',\nifnull(vm.android_version,'no_data') as 'Android',\nifnull(vm.arch,'no_data') as 'arch',\nifnull(vm.deviceid,'no_data') as 'DeviceId',\nifnull(vm.pogo,'no_data') as 'pogo',\nifnull(vm.vmapper,'no_data') as 'vmapper',\nifnull(vm.magisk,'no_data') as 'magisk',\nvmapperSh as 'vmapper.sh',\nvmapper49 as '49vm',\nvmwatchdog56 as '56wd',\nwhversion as 'wh',\nproxyinfo as 'Proxy',\nifnull(playstoreenabled, 'no_data') as 'Playstore Update',\nifnull(playstore,'no_data') as 'playstore Ver',\ntemperature as 'temp',\nconvert_tz(timestamp,@@session.time_zone, '+00:00') as 'lastUpdate'\n-- timestamp as 'lastUpdate'\nFROM ATVsummary vm\nORDER BY vm.deviceName ASC",
          "refId": "A",
