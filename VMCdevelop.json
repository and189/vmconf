{
    "vm: enable vmconf develop":
    [
        {
            "TYPE": "jobType.PASSTHROUGH",
            "SYNTAX": "su -c 'touch /sdcard/useVMCdevelop && /system/bin/vmapper.sh'"
        }
    ],

    "vm: disable vmconf develop":
    [
        {
            "TYPE": "jobType.PASSTHROUGH",
            "SYNTAX": "su -c 'rm -f /sdcard/useVMCdevelop && /system/bin/vmapper.sh'"
        }
    ]
}
