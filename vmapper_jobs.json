{
        "10: setting overview":
        [
        {
                "TYPE": "jobType.PASSTHROUGH",
                "SYNTAX": "echo \" mitm=$(ps | grep -e de.goldjpg.vmapper -e com.mad.pogodroid | awk -F. '{ print $NF }') | Vmapper=$(dumpsys package de.goldjpg.vmapper | grep versionName | head -n1 | sed 's/ *versionName=//') | VMscript=$(grep '# version' /system/bin/vmapper.sh | awk '{ print $NF }') VM_Autoupdate=$([ -f /sdcard/disableautovmapperupdate ] && echo disabled || echo enabled) \" && awk -F'[\"]+' '/value/{print \"VM_\"$2 \"=\" $4}' /data/data/de.goldjpg.vmapper/shared_prefs/config.xml |tr '\n' ' ' && awk -F'[<>\"]+' '/string/{print \"VM_\"$3 \"=\" $4}' /data/data/de.goldjpg.vmapper/shared_prefs/config.xml |tr '\n' ' '"
        }
        ],

        "01: install vmapper (autoupdate disabled)":
        [
        {
                "TYPE": "jobType.PASSTHROUGH",
                "SYNTAX": "su -c 'mount -o remount,rw /system && /system/bin/curl -L -o /sdcard/vmapper_conf -k -s LinkToYourSecretHideoutForDownloadFiles/vmapper_conf  && /system/bin/curl -L -o /system/bin/vmapper.sh -k -s https://raw.githubusercontent.com/dkmur/vmconf/main/vmapper.sh && chmod +x /system/bin/vmapper.sh && /system/bin/vmapper.sh -iv'"
        }
        ],

        "03: install vmapper from wizzard (autoupdate enabled)":
        [
        {
                "TYPE": "jobType.PASSTHROUGH",
                "SYNTAX": "su -c 'mount -o remount,rw /system && /system/bin/curl -L -o /sdcard/vmapper_conf -k -s LinkToYourSecretHideoutForDownloadFiles/vmapper_conf  && /system/bin/curl -L -o /system/bin/vmapper.sh -k -s https://raw.githubusercontent.com/dkmur/vmconf/main/vmapper.sh && chmod +x /system/bin/vmapper.sh && /system/bin/vmapper.sh -ivw'"
        }
        ],

        "02: install vmapper (autoupdate disabled) and pogo update":
        [
        {
                "TYPE": "jobType.PASSTHROUGH",
                "SYNTAX": "su -c 'mount -o remount,rw /system && /system/bin/curl -L -o /sdcard/vmapper_conf -k -s LinkToYourSecretHideoutForDownloadFiles/vmapper_conf  && /system/bin/curl -L -o /system/bin/vmapper.sh -k -s https://raw.githubusercontent.com/dkmur/vmconf/main/vmapper.sh && chmod +x /system/bin/vmapper.sh && /system/bin/vmapper.sh -iv -up'"
        }
        ],

        "23: update configuration script (vmapper.sh)":
        [
        {
                "TYPE": "jobType.PASSTHROUGH",
                "SYNTAX": "su -c '/system/bin/vmapper.sh -us'"
        }
        ],

        "24: update script configuration file (vmapper_conf)":
        [
        {
                "TYPE": "jobType.PASSTHROUGH",
                "SYNTAX": "su -c '/system/bin/vmapper.sh -uvc'"
        }
        ],

        "25: update config.xml (including update vmapper_conf)":
        [
        {
                "TYPE": "jobType.PASSTHROUGH",
                "SYNTAX": "su -c '/system/bin/vmapper.sh -uvx'"
        }
        ],

        "20: update vmapper":
        [
        {
                "TYPE": "jobType.PASSTHROUGH",
                "SYNTAX": "su -c '/system/bin/vmapper.sh -uv'"
        }
        ],

        "22: update pogo":
        [
        {
                "TYPE": "jobType.PASSTHROUGH",
                "SYNTAX": "su -c '/system/bin/vmapper.sh -up'"
        }
        ],

        "21: update vmapper and pogo":
        [
        {
                "TYPE": "jobType.PASSTHROUGH",
                "SYNTAX": "su -c '/system/bin/vmapper.sh -uv -up'"
        }
        ],

        "30: add script (only needed when vmapper install was done before repo was created)":
        [
        {
                "TYPE": "jobType.PASSTHROUGH",
                "SYNTAX": "su -c 'mount -o remount,rw /system && /system/bin/curl -L -o /sdcard/vmapper_conf -k -s LinkToYourSecretHideoutForDownloadFiles/vmapper_conf  && /system/bin/curl -L -o /system/bin/vmapper.sh -k -s https://raw.githubusercontent.com/dkmur/vmconf/main/vmapper.sh && chmod +x /system/bin/vmapper.sh'"
        }
        ],

        "13: vmapper auto-update: enable": [
        {
                "TYPE": "jobType.PASSTHROUGH",
                "SYNTAX": "su -c 'rm /sdcard/disableautovmapperupdate'"
        }
        ],

        "14: vmapper auto-update: disable": [
        {
                "TYPE": "jobType.PASSTHROUGH",
                "SYNTAX": "su -c 'touch /sdcard/disableautovmapperupdate'"
        }
        ],

        "31: add vmapper autoupdate YET disabled (only needed when vmapper install was done before autoupdate was created)":
        [
        {
                "TYPE": "jobType.PASSTHROUGH",
                "SYNTAX": "su -c 'mount -o remount,rw /system && /system/bin/curl -L -o /system/etc/init.d/55vmapper -k -s https://raw.githubusercontent.com/dkmur/vmconf/main/55vmapper && chmod +x /system/etc/init.d/55vmapper && touch /sdcard/disableautovmapperupdate'"
        }
        ],

        "11: disable VM and enable PD daemon":
        [
        {
                "TYPE": "jobType.PASSTHROUGH",
                "SYNTAX": "su -c '/system/bin/vmapper.sh -svp'"
        }
        ],

        "12: enable VM and disable PD daemon":
        [
        {
                "TYPE": "jobType.PASSTHROUGH",
                "SYNTAX": "su -c '/system/bin/vmapper.sh -spv'"
        }
        ]
}
